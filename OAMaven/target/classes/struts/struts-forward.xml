<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="forward" namespace="/" extends="privilegeOA">
		<action name="forwardAction_*" class="com.wx.oa.action.ForwardAction" method="{1}">
			<result name="top">WEB-INF/jsp/frame/top.jsp</result>
			<result name="bottom">WEB-INF/jsp/frame/footer.jsp</result>
			<result name="left">WEB-INF/jsp/frame/left.jsp</result>
			<result name="right">WEB-INF/jsp/frame/right.jsp</result>
		</action>
	</package>
</struts>
